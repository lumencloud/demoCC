<mvc:View
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:table="sap.ui.table"
    xmlns:rowmodes="sap.ui.table.rowmodes"
    width="100%"
    height="100%"
    displayBlock="true"
    controllerName="bix.card.planOiNonMMTable.Main"
>
    <Panel
        expandable="false"
        expanded="true"
        width="auto"
        height="100%"
        class="sapUiNoContentPadding"
    >
        
        <content>
        <VBox width="100%" height="100%">
        <OverflowToolbar style="Clear" height="2.5rem">
                        <ToolbarSpacer />                        
                        <HBox class="custom-select-box" width="30%" justifyContent="End">
                            <Select
                                id="detailSelect"
                                change=".onUiChange"
                                width="30%"                            
                                selectedKey="{uiModel>/tableKind}"
                                items="{path: 'selectModel>/'}"
                            >
                                <items>
                                    <core:ListItem
                                        key="{selectModel>sub_key}"
                                        text="{selectModel>sub_text}"
                                    />
                                </items>
                            </Select>
                        </HBox>
                    </OverflowToolbar>

        <VBox id="planOiNonMMBox1" busyIndicatorDelay="0" visible="{=${uiModel>/tableKind} === 'account'}" class="custom-vbox-wrap-table" height="100%">
            <table:Table
                id="planOiNonMMTable1"
                rows="{path:'oAccountTableModel>/'}"
                class="custom-table-container custom-table-header custom-table-body custom-table-first-emphasis custom-uiTable-selection custom-inherit-height custom-table-rounding-border"
                selectionMode="None"
                fixedColumnCount="2"
                busyIndicatorDelay="0"
                visibleRowCountMode="Auto"
            >

            <table:layoutData>
                        <FlexItemData growFactor="1"/>
                    </table:layoutData>
           

                <table:columns>
                    <table:Column
                        hAlign="Center"
                        width="10rem"
                        resizable="false"
                        name="type"
                    >
                        <table:multiLabels>
                            <Label/>
                            <Label
                                text="Account"
                                wrapping="true"
                            />
                            
                        </table:multiLabels>
                        <table:template>
                            <Text emptyIndicatorMode="On"
                                text="{oAccountTableModel>account_nm}"
                                tooltip="{oAccountTableModel>account_nm}"
                                wrapping="false"
                                width="100%"
                                textAlign="Center"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column
                        hAlign="Center"
                        width="5rem"
                        resizable="false"
                        name="type"
                    >
                        <table:multiLabels>
                            <Label/>
                            <Label
                                text="구분"
                                wrapping="true"
                            />
                            
                        </table:multiLabels>
                        <table:template>
