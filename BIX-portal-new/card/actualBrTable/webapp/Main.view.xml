<mvc:View
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:table="sap.ui.table"
    xmlns:rowmodes="sap.ui.table.rowmodes"
    width="100%"
    height="100%"
    displayBlock="true"
    controllerName="bix.card.actualBrTable.Main"
>
    <Panel
        expandable="false"
        expanded="true"
        width="auto"
        height="100%"
        class="sapUiNoContentPadding"
    >
    <content>
    <VBox width="100%" height="100%">        
            <VBox fitContainer="true"
                visible="{=${uiModel>/tableKind} === 'org'}"
                id="actualBrBox1"
                busyIndicatorDelay="0"
                            class="custom-vbox-wrap-table"
                            height="100%"

                 >
                <table:Table
                    id="actualBrTable1"
                    rows="{path:'oOrgTableModel>/'}"
                    class="custom-table-container custom-table-header custom-table-body custom-table-first-emphasis 
                    custom-uiTable-selection custom-inherit-height custom-table-rounding-border custom-inherit-height"
                    selectionMode="Single" 
                    selectionBehavior="RowOnly" 
                    firstVisibleRowChanged="onFirstVisibleRowChanged"
                    rowSelectionChange="onRowSelectionChange"
                    fixedColumnCount="2"
                    busyIndicatorDelay="0"
                    visibleRowCountMode="Auto"
                >
                    <table:layoutData>
                        <FlexItemData growFactor="1" />
                    </table:layoutData>

                    <table:extension>
                        <OverflowToolbar style="Clear" height="2.5rem">
                        </OverflowToolbar>
                    </table:extension>

                    <table:columns>
                        <table:Column
                            hAlign="Center"
                            width="auto"
                            resizable="false"
                            name="type"
                        >
                            <table:multiLabels>
                                <Label />
                                <Label
                                    text="조직"
                                    wrapping="true"
                                />
                            </table:multiLabels>
                            <table:template>
                                <Text emptyIndicatorMode="On"
                                    text="{oOrgTableModel>org_name}"
                                    tooltip="{oOrgTableModel>org_name}"
                                    wrapping="false"
                                    width="100%"
                                    textAlign="Center"
                                />
                            </table:template>
                        </table:Column>

                        <table:Column
                            hAlign="Center"
                            width="7rem"
                            resizable="false"
                            name="type"
                        >
                            <table:multiLabels>
                                <Label />
                                <Label
                                    text="구분"
                                    wrapping="true"
                                />
                            </table:multiLabels>
                            <table:template>
                                <Text emptyIndicatorMode="On"
                                    text="{oOrgTableModel>type}"
                                    tooltip="{oOrgTableModel>type}"
                                    wrapping="false"
                                    width="100%"
                                    textAlign="Center"
                                />
                            </table:template>
                        </table:Column>

                        <table:Column
                            hAlign="Center"
                            width="5rem"
                            resizable="false"
                            name="type"
                        >
                            <table:multiLabels>
                                <Label />
                                <Label
                                    text="목표"
                                    wrapping="true"
                                />
                            </table:multiLabels>
                            <table:template>
                                <Text emptyIndicatorMode="On"
                                    text="{
                                    parts:[                                                
                                        {path:'oOrgTableModel>target_curr_y_value', targetType: 'any'}
                                    ],
                                    formatter: '.onFormatPerformance'
                                }"
                                    tooltip="{     
                                    parts:[                                                         
                                        {path:'oOrgTableModel>target_curr_y_value', targetType: 'any'},                                                        
                                        {value:''},         
                                        {value:'targetTooltip'}    
                                    ],     
                                    formatter: '.onFormatPerformance'                                 
                                }"
                                    wrapping="false"
                                    width="100%"
                                    textAlign="End"
                                />
                            </table:template>
                        </table:Column>

                        <table:Column
                            hAlign="Center"
                            headerSpan="3"
                            resizable="false"
                            width="7rem"
                        >
                            <table:multiLabels>
                                <Label
                                    text="당월 실적"
                                    wrapping="true"
                                />
                                <Label
                                    text="당월"
                                    textAlign="Center"
                                    width="100%"
                                />
                            </table:multiLabels>
                            <table:template>
                                <Text emptyIndicatorMode="On"
                                    text="{     
                                            parts:[         
                                                {path:'oOrgTableModel>actual_curr_ym_value', targetType: 'any'}
                                            ],     
                                            formatter: '.onFormatPerformance' }"
                                    tooltip="{     
                                        parts:[     
                                            {path:'oOrgTableModel>actual_curr_ym_value', targetType: 'any'},  
                                            {value:''},         
                                            {value:'tooltip'}
                                        ],     
                                        formatter: '.onFormatPerformance'                                 
                                        }"
                                    wrapping="false"
                                    width="100%"
                                    textAlign="End"
                                />
                            </table:template>
                        </table:Column>

                        <table:Column
                            hAlign="Center"
                            width="7rem"
                            resizable="false"
                        >
                            <table:multiLabels>
                                <Label
                                    text="당월 실적"
                                    wrapping="true"
                                />
                                <Label
                                    text="전년 동기"
                                    textAlign="Center"
                                    width="100%"
                                />
                            </table:multiLabels>
                            <table:template>
                                <Text emptyIndicatorMode="On"
                                        text="{     
                                        parts:[        
                                            {path:'oOrgTableModel>actual_last_ym_value', targetType: 'any'}
                                        ],     
                                        formatter: '.onFormatPerformance'
                                    }"
                                        tooltip="{    
                                        parts:[       
                                            {path:'oOrgTableModel>actual_last_ym_value', targetType: 'any'},   
                                            {value:''},         
                                            {value:'tooltip'}
                                        ],     
                                        formatter: '.onFormatPerformance'                                 
                                    }"
                                        wrapping="false"
                                        width="100%"
                                        textAlign="End"
                                    />
                            </table:template>
                        </table:Column>

                        <table:Column
                            hAlign="Center"
                            width="7rem"
                            resizable="false"
                        >
                            <table:multiLabels>
                                <Label
                                    text="당월 실적"
                                    wrapping="true"
                                />
                                <Label
                                    text="GAP"
                                    textAlign="Center"
                                    width="100%"
                                />
                            </table:multiLabels>
                            <table:template>
                                <Text emptyIndicatorMode="On"
                                    text="{
                                            parts:[ 
                                                {path:'oOrgTableModel>actual_curr_ym_value', targetType: 'any'}, 
                                                {path:'oOrgTableModel>actual_last_ym_value', targetType: 'any'}
                                            ],
                                            formatter: '.onFormatPerformance'
                                            }"
                                        tooltip="{
                                            parts:[ 
                                                {path:'oOrgTableModel>actual_curr_ym_value', targetType: 'any'}, 
                                                {path:'oOrgTableModel>actual_last_ym_value', targetType: 'any'}, 
                                                {value:'tooltip'}
                                            ],
                                            formatter: '.onFormatPerformance'                                
                                            }"
                                        wrapping="false"
                                        width="100%"
                                        textAlign="End"
                                />
                            </table:template>
                        </table:Column>

                        <table:Column
                            hAlign="Center"
                            width="7rem"
                            resizable="false"
                            headerSpan="3"
                        >
                            <table:multiLabels>
                                <Label
                                    text="진척도"
                                    wrapping="true"
                                />
                                <Label
                                    text="당월"
                                    textAlign="Center"
                                    width="100%"
                                />
                            </table:multiLabels>
                            <table:template>
                                <Text emptyIndicatorMode="On"
                                    text="{
                                            parts:[ 
                                                {path:'oOrgTableModel>actual_curr_ym_rate', targetType: 'any'}
                                            ],
                                            formatter: '.onFormatPerformance'
                                            }"
                                        tooltip="{
                                            parts:[ 
                                                {path:'oOrgTableModel>actual_curr_ym_rate', targetType: 'any'}, 
                                                {value:''}, 
                                                {value:'tooltip'}
                                            ],
                                            formatter: '.onFormatPerformance'                                
                                            }"
                                        wrapping="false"
                                        width="100%"
                                        textAlign="End"
                                />
                            </table:template>
                        </table:Column>

                        <table:Column
                            hAlign="Center"
                            width="7rem"
                            resizable="false"
                        >
                            <table:multiLabels>
                                <Label
                                    text="진척도"
                                    wrapping="true"
                                />
                                <Label
                                    text="전년 동기"
                                    textAlign="Center"
                                    width="100%"
                                />
                            </table:multiLabels>
                            <table:template>
                                <Text emptyIndicatorMode="On"
                                    text="{
                                            parts:[ 
                                                {path:'oOrgTableModel>actual_last_ym_rate', targetType: 'any'}
                                            ],
                                            formatter: '.onFormatPerformance'
                                        }"
                                        tooltip="{
                                            parts:[ 
                                                {path:'oOrgTableModel>actual_last_ym_rate', targetType: 'any'}, 
                                                {value:''}, 
                                                {value:'tooltip'}
                                            ],
                                            formatter: '.onFormatPerformance'                                
                                        }"
                                        wrapping="false"
                                        width="100%"
                                        textAlign="End"
                                />
                            </table:template>
                        </table:Column>

                        <table:Column
                            hAlign="Center"
                            width="7rem"
                            resizable="false"
                        >
                            <table:multiLabels>
                                <Label
                                    text="진척도"
                                    wrapping="true"
                                />
                                <Label
                                    text="GAP"
                                    textAlign="Center"
                                    width="100%"
                                />
                            </table:multiLabels>
                            <table:template>
                                <Text emptyIndicatorMode="On"
                                    text="{
                                        parts:[ 
                                            {path:'oOrgTableModel>actual_curr_ym_rate', targetType: 'any'}, 
                                            {path:'oOrgTableModel>actual_last_ym_rate', targetType: 'any'}
                                        ],
                                        formatter: '.onFormatPerformance'
                                        }"
                                        tooltip="{
                                            parts:[ 
                                                {path:'oOrgTableModel>actual_curr_ym_rate', targetType: 'any'},
                                                {path:'oOrgTableModel>actual_last_ym_rate', targetType: 'any'}, 
                                                {value:'tooltip'}
                                            ],
                                            formatter: '.onFormatPerformance'                                
                                        }"
                                        wrapping="false"
                                        width="100%"
                                        textAlign="End"
                                />
                            </table:template>
                        </table:Column>
                    </table:columns>
                </table:Table>
            </VBox>
        </VBox>
        </content>
    </Panel>
</mvc:View>
