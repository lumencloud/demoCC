		IFNULL(TOTAL_M6_EMP,0),
		IFNULL(TOTAL_M7_EMP,0),
		IFNULL(TOTAL_M8_EMP,0),
		IFNULL(TOTAL_M9_EMP,0),
		IFNULL(TOTAL_M10_EMP,0),
		IFNULL(TOTAL_M11_EMP,0),
		IFNULL(TOTAL_M12_EMP,0),
		IFNULL(AVG_M1_AMT,0),
		IFNULL(AVG_M2_AMT,0),
		IFNULL(AVG_M3_AMT,0),
