		YEAR,
		LPAD(MONTH,2,'0') AS MONTH,
		CCORG_CD,
		BILL_M1_AMT,
		BILL_M2_AMT,
		BILL_M3_AMT,
		BILL_M4_AMT,
		BILL_M5_AMT,
		BILL_M6_AMT,
		BILL_M7_AMT,
		BILL_M8_AMT,
