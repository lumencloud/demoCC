VIEW COMMON_MIS_GET_PRJ_DESCENDANT(IN ID NVARCHAR(20)) AS

SELECT 
MIS_PROJECT.PRJ_NO,
MIS_PROJECT.SELL_SLS_CNRC_NO,
MIS_COM_ORG.ORG_TP_RCID,
MIS_COM_ORG.ID,
MIS_PROJECT.RODR_ESMT_YM
	
FROM (
	SELECT *
	FROM COMMONSERVICE_MIS_GET_ORG_DESCENDANT(:ID, NULL)
) AS MIS_COM_ORG
			
JOIN COMMONSERVICE_MIS_PROJECT AS MIS_PROJECT ON MIS_COM_ORG.ID = MIS_PROJECT.SALE_ORG_RID