/**
* 조직 ID의 한글 이름
* @ID : 조직의 ID 
*/
FUNCTION F_MIS_GET_ORG_KOR_NM (
	ID NVARCHAR(50)
)

RETURNS VAR_ORG_KOR_NM STRING LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER AS
BEGIN
	IF :ID IS NULL THEN	-- ID가 NULL이면 NULL 반환
		VAR_ORG_KOR_NM = NULL;
	ELSE 
		SELECT ORG_KOR_NM
		INTO VAR_ORG_KOR_NM
		FROM (
			SELECT ORG_KOR_NM
			FROM COMMON_MIS_COM_ORG
			WHERE ID = :ID
		);	
	END IF;
END;