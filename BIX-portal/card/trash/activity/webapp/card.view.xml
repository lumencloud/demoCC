<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:viz="sap.viz.ui5.controls" 
	xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
    xmlns:viz.data="sap.viz.ui5.data"
    xmlns:form="sap.ui.layout.form"
    xmlns:w="sap.ui.integration.widgets"
	xmlns:card="sap.f.cards"
    controllerName="bix.card.activity.card"
    xmlns:f="sap.f"
    height="100%"
	width="100%"
>
    <Panel 
        height="500px"
        width="100%">
        <VBox justifyContent="Center" height="100%">
            <w:Card manifest="../bix/card/homeVolatilityCost/manifest.json">
                <w:customData>
                    <card:CardBadgeCustomData value="New" announcementText="Card was newly added."/>
                </w:customData>
                <w:layoutData>
                    <f:GridContainerItemLayoutData rows="4" columns="12" />
                </w:layoutData>
            </w:Card>

            <!-- <core:HTML
            content="&lt;canvas id=&quot;activity&quot; &gt;&lt;/canvas&gt;"/> -->
        </VBox>
    </Panel>

</mvc:View>
