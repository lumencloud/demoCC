                </table:multiLabels>
                <table:template>
                    <Text text="" 
                    tooltip=""
                    wrapping="false" width="100%" textAlign="End" emptyIndicatorMode="On"/>
                </table:template>
            </table:Column>

            <table:Column hAlign="Center" width="7rem" resizable="false">
                <table:multiLabels>
                    <Label text=""/>
