<mvc:View
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:table="sap.ui.table"
    xmlns:rowmodes="sap.ui.table.rowmodes"
    width="100%"
    displayBlock="true"
    controllerName="bix.card.plPlanPipelineStepTable.Main"
    
>

     <table:Table
        id="plPlanPipelineStepTable"
        class="custom-table-container custom-table-header custom-table-body custom-table-first-emphasis custom-uiTable-selection custom-inherit-height custom-selection-radius" 
        selectionMode="Single" 
        selectionBehavior="RowOnly" 
        rowSelectionChange="onRowSelectionChange"
        enableColumnReordering="false"
        fixedColumnCount="1"
        busyIndicatorDelay="0"
        >

        <table:extension>
            <OverflowToolbar width="100%" height="3rem" style="Standard">                
                <ToolbarSpacer />
            </OverflowToolbar>
        </table:extension>
        
        <table:rowMode>
            <rowmodes:Fixed rowCount="8" />
        </table:rowMode>

        <table:columns>            
            <table:Column hAlign="Center" headerSpan="12" resizable="false">
                <table:multiLabels>
                    <Label text="Pipeline 상세 - 단계별 View" wrapping="true"/>
                    <Label text=""/>
                    <Label text="Lead"/>
                </table:multiLabels>
                <table:template>
                    <Text text="" 
                    tooltip=""
                    wrapping="false" width="100%" textAlign="End" emptyIndicatorMode="On"/>
                </table:template>
            </table:Column>
            
            <table:Column hAlign="Center" width="7rem" resizable="false">
                <table:multiLabels>
                    <Label text=""/>
                    <Label text="잠재사업"/>
                    <Label text="Identified &#10; (10%)" wrapping="true" textAlign="Center"/>
                </table:multiLabels>
                <table:template>
                    <Text text="" 
                    tooltip=""
                    wrapping="false" width="100%" textAlign="End" emptyIndicatorMode="On"/>
                </table:template>
            </table:Column>
            
            <table:Column hAlign="Center" width="7rem" resizable="false">
                <table:multiLabels>
                    <Label text=""/>
                    <Label text=""/>
                    <Label text="Identified- &#10; Remind(20%)" wrapping="true" textAlign="Center"/>
                </table:multiLabels>
                <table:template>
                    <Text text="" 
                    tooltip=""
                    wrapping="false" width="100%" textAlign="End" emptyIndicatorMode="On"/>
                </table:template>
            </table:Column>
            
            <table:Column hAlign="Center" width="7rem" resizable="false">
                <table:multiLabels>
                    <Label text=""/>
                    <Label text=""/>
                    <Label text="Registered &#10;(35%)" wrapping="true" textAlign="Center"/>
                </table:multiLabels>
                <table:template>
                    <Text text="" 
                    tooltip=""
                    wrapping="false" width="100%" textAlign="End" emptyIndicatorMode="On"/>
                </table:template>
            </table:Column>

            <table:Column hAlign="Center" width="7rem" resizable="false">
                <table:multiLabels>
                    <Label text=""/>
                    <Label text="영업단계"/>
                    <Label text="Validated &#10;(40%)" wrapping="true" textAlign="Center"/>
                </table:multiLabels>
                <table:template>
                    <Text text="" 
                    tooltip=""
                    wrapping="false" width="100%" textAlign="End" emptyIndicatorMode="On"/>
                </table:template>
            </table:Column>

            <table:Column hAlign="Center" width="7rem" resizable="false">
                <table:multiLabels>
                    <Label text=""/>
                    <Label text=""/>
                    <Label text="Qualified &#10; (50%)" wrapping="true" textAlign="Center"/>
                </table:multiLabels>
                <table:template>
                    <Text text="" 
                    tooltip=""
                    wrapping="false" width="100%" textAlign="End" emptyIndicatorMode="On"/>
                </table:template>
            </table:Column>

            <table:Column hAlign="Center" width="7rem" resizable="false">
                <table:multiLabels>
                    <Label text=""/>
                    <Label text="제안"/>
