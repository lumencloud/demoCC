{
  "_version": "1.65.0",
  "sap.app": {
    "id": "bix.pl.performance",
    "type": "application",
    "i18n": {
      "bundleUrl": "../../bix/portal/i18n/i18n.properties",
      "supportedLocales": ["ko","en"],
      "fallbackLocale": "ko"
    },
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.16.5",
      "toolsId": "3df907c1-95f6-455b-beaf-196522c4bd9f"
    },
    "dataSources": {
      "v4Service": {
        "uri": "../../odata/v4/common/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      },
      "plService": {
        "uri": "../../odata/v4/pl_api/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      },
      "plService_old": {
        "uri": "../../odata/v4/pl/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      },
      "v2Service": {
        "uri": "../../odata/v2/cm/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.134.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleUrl": "../../bix/portal/i18n/i18n.properties",
          "supportedLocales": ["ko","en"],
          "fallbackLocale": "ko"
        }
      },
      "": {
        "dataSource": "v4Service",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "v2": {
        "dataSource": "v2Service",
        "preload": true,
        "settings": {
          "useBatch": true,
          "withCredentials": true,
          "defaultCountMode": "Inline"
        }
      },
      "pl2": {
        "dataSource": "plService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "pl": {
        "dataSource": "plService_old",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "bix.pl.performance.view"
      },
      "routes": [
        {
          "name": "RoutePL",
          "pattern": ":?query:",
          "target": [
            "TargetPL",
            "TargetSale"
          ]
        },
        {
          "name": "RoutePLChart",
          "pattern": "chart",
          "target": [
            "TargetPL",
            "TargetChart"
          ]
        },
        {
          "name": "RouteSGA",
          "pattern": "sga",
          "target": [
            "TargetPL",
            "TargetSGA"
          ]
        },
        {
          "name": "RouteSale",
          "pattern": "sale",
          "target": [
            "TargetPL",
            "TargetSaleDetail"
          ]
        }
      ],
      "targets": {
        "TargetPL": {
          "id": "PL",
          "name": "PL",
          "controlId": "2depth"
        },
        "TargetChart": {
          "id": "PerformanceChart",
          "name": "PerformanceChart",
          "controlId": "3depth"
        },
        "TargetSGA": {
          "id": "SGA",
          "name": "SGA",
          "controlId": "3depth"
        },
        "TargetSale": {
          "id": "Sale",
          "name": "Sale",
          "controlId": "3depth"
        },
        "TargetSaleDetail": {
          "id": "SaleDetail",
          "name": "SaleDetail",
          "controlId": "3depth"
        }
      }
    },
    "rootView": {
      "viewName": "bix.pl.performance.view.Splitter",
      "type": "XML",
      "id": "App"
    }
  }
}
