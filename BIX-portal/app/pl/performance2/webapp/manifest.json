{
  "_version": "1.65.0",
  "sap.app": {
    "id": "bix.pl.performance2",
    "type": "application",
    "i18n": {
      "bundleUrl": "../../bix/portal/i18n/i18n.properties",
      "supportedLocales": [
        "ko",
        "en"
      ],
      "fallbackLocale": "ko"
    },
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.16.5",
      "toolsId": "3df907c1-95f6-455b-beaf-196522c4bd9f"
    },
    "dataSources": {
      "v4Service": {
        "uri": "../../odata/v4/common/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      },
      "plService": {
        "uri": "../../odata/v4/pl-api/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      },
      "sgaService": {
        "uri": "../../odata/v4/sga-api/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      },
      "rspService": {
        "uri": "../../odata/v4/rsp-api/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      },
      "plService_old": {
        "uri": "../../odata/v4/pl/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      },
      "v2Service": {
        "uri": "../../odata/v2/common/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      },
      "cmService": {
        "uri": "../../odata/v4/cm/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      },
      "plApiService": {
        "uri": "../../odata/v4/pl_api/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.134.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleUrl": "../../bix/portal/i18n/i18n.properties",
          "supportedLocales": [
            "ko",
            "en"
          ],
          "fallbackLocale": "ko"
        }
      },
      "": {
        "dataSource": "v4Service",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "v2": {
        "dataSource": "v2Service",
        "preload": true,
        "settings": {
          "useBatch": true,
          "withCredentials": true,
          "defaultCountMode": "Inline"
        }
      },
      "pl2": {
        "dataSource": "plService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "pl": {
        "dataSource": "plService_old",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "sga": {
        "dataSource": "sgaService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "rsp": {
        "dataSource": "rspService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },      
      "cm": {
        "dataSource": "cmService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "pl_api": {
        "dataSource": "plApiService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "bix.pl.performance2.view"
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":page:/:pageView:/:detail:/:detailType:",
          "target": [
            "TargetSplitter"
          ]
        }
      ],
      "targets": {
        "TargetSplitter": {
          "id": "Splitter",
          "name": "Splitter"
        }
      }
    },
    "rootView": {
      "viewName": "bix.pl.performance2.view.App",
      "type": "XML",
      "id": "App"
    }
  }
}