{
    "agent_id": "report_agent",
    "processing_flow": {
      "flow_type": "langchain_sequential",
      "execution_mode": "sequential",
      "steps": [
        {
          "step_id": "get_and_filter_data",
          "step_sequence": 1,
          "component": {
            "id": "tool_01",
            "type": "tool",
            "description": "데이터 조회 및 필터링",
            "tool_file": "data_fetch_tool",
            "input_variables": {
              "function_name": "$input:function_name",
              "function_params": "$input:function_params",
              "filter_type": "$input:filter_type"
            },
            "output_variable": "drilldown_data"
          }
        },
        {
          "step_id": "analysis_objectives",
          "step_sequence": 2,
          "component": {
            "id": "prompt_01",
            "type": "prompt",
            "description": "분석목표정의",
            "template": {
              "name": "report_analysis-goal",
              "scenario": "foundation-models",
              "version": "0.0.1"
            },
            "model_config": {
              "model_id": "model_01",
              "model_name": "gpt-4o",
              "model_params": {
                  "temperature": 0.3,
                  "max_tokens": 4000
              },
              "model_version": "2024-11-20"
            },
            "input_variables":{
              "drilldown_data": "$var:drilldown_data",
              "selected_info": "$input:selected_info",
              "key_definition": "$input:key_definition"
            },
            "output_variable": "report_key_point"
          }
        },
        {
          "step_id": "create_report",
          "step_sequence": 3,
          "component": {
            "id": "prompt_02",
            "type": "prompt",
            "description": "리포트생성",
            "template": {
              "name": "report_report-generation",
              "scenario": "foundation-models",
              "version": "0.0.1"
            },
            "model_config": {
              "model_id": "model_02",
              "model_name": "gpt-4.1-mini",
              "model_params": {
                  "temperature": 0.3
              },
              "model_version": "2025-04-14"
            },
            "input_variables":{
              "drilldown_data": "$var:drilldown_data",
              "selected_info": "$input:selected_info",
              "key_definition": "$input:key_definition",
              "report_key_point": "$var:report_key_point"
            },
            "output_variable": "final"
          }
        }
      ]
    }
  }