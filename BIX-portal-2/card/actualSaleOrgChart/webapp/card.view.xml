<mvc:View
    controllerName="bix.card.actualSaleOrgChart.card"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    width="100%"
    height="100%"
    displayBlock="true"
>

   
    <Panel
        expandable="false"
        expanded="true"
        width="100%"
        height="auto"
    >   
           
        <content>
            <VBox width="100%">
                <OverflowToolbar width="100%" height="2.5rem" class="custom-overflow-toolbar">                   
                    <ToolbarSpacer/>
                    <HBox class="custom-select-box" width="30%" justifyContent="End">
                        <Select
                            change=""
                            width="100%"
                            selectedKey="{uIModel>/tableKind}"
                            items="{path: 'SelectModel>/'}"
                        >
                            <items>
                                <core:ListItem
                                    key="{SelectModel>key}"
                                    text="{SelectModel>name}"
                                />
                            </items>
                        </Select>
                    </HBox>
                </OverflowToolbar>
            </VBox>
            <VBox fitContainer="true">
                <FlexBox
                    alignItems="Center"
                    justifyContent="Center"
                    fitContainer="true"
                >
                    <core:HTML id="html0" />
                </FlexBox>

                
            </VBox>
        </content>
    </Panel>
</mvc:View>
